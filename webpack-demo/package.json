{
  "name": "webpack-demo",
  "version": "1.0.1",
  "description": "webpack demo files for npm-notes",
  "main": "index.js",
  "config": {
    "port": "8080"
  },
  "scripts": {
    "test": "echo \"No test specified yet. Check out later info on jest.\" && exit 1",
    "build": "webpack --config wp.html_config.js",
    "dev": "[ ! -z "$npm_config_port" ] && webpack-dev-server --config wp.html_config.js --mode development --port $npm_config_port || npm run dev --port=$npm_package_config_port"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JR-Mitchell/npm-notes.git"
  },
