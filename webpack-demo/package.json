  "bugs": {
    "url": "https://github.com/JR-Mitchell/npm-notes/issues"
  },
  "homepage": "https://github.com/JR-Mitchell/npm-notes#readme",
  "devDependencies": {
    "html-webpack-plugin": "^4.3.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
